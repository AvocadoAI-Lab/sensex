{
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "@timestamp": {
              "gte": "now-12h",
              "lte": "now"
            }
          }
        }
      ],
      "should": [
        {
          "match": {
            "rule.groups": "authentication_failed"
          }
        },
        {
          "match": {
            "rule.groups": "authentication_success"
          }
        }
      ],
      "minimum_should_match": 1,
      "filter": [
        {
          "range": {
            "rule.level": {
              "gte": 7
            }
          }
        }
      ]
    }
  },
  "size": 50,
  "sort": [
    {
      "@timestamp": {
        "order": "desc"
      }
    }
  ],
  "_source": [
    "@timestamp",
    "rule.description",
    "rule.level",
    "agent.name",
    "agent.id",
    "data.srcip",
    "data.srcuser",
    "data.dstuser"
  ]
}
