{
  "size": 0,
  "aggs": {
    "level_stats": {
      "terms": {
        "field": "rule.level",
        "size": 15
      }
    },
    "group_stats": {
      "terms": {
        "field": "rule.groups",
        "size": 15
      }
    },
    "agent_stats": {
      "terms": {
        "field": "agent.name",
        "size": 15
      }
    },
    "alerts_over_time": {
      "date_histogram": {
        "field": "@timestamp",
        "calendar_interval": "1h"
      }
    }
  },
  "query": {
    "range": {
      "@timestamp": {
        "gte": "now-24h",
        "lte": "now"
      }
    }
  }
}
